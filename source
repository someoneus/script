local player = game.Players.LocalPlayer

local function distortName(str, percentage)
    local length = #str
    local numToShuffle = math.max(1, math.floor(length * percentage))  

    local letters = {}  
    for i = 1, length do
        table.insert(letters, str:sub(i, i))
    end

    local shuffleIndexes = {}
    while #shuffleIndexes < numToShuffle do
        local randIndex = math.random(1, length)
        if not table.find(shuffleIndexes, randIndex) then
            table.insert(shuffleIndexes, randIndex)
        end
    end

    local selectedChars = {}
    for _, i in ipairs(shuffleIndexes) do
        table.insert(selectedChars, letters[i])
    end

    for i = #selectedChars, 2, -1 do
        local j = math.random(1, i)
        selectedChars[i], selectedChars[j] = selectedChars[j], selectedChars[i]  -- Shuffle selected letters
    end

    for index, i in ipairs(shuffleIndexes) do
        letters[i] = selectedChars[index]
    end

    local finalName = {}
    for i = 1, #letters do
        table.insert(finalName, letters[i])
        if math.random() < 0.3 then  
            table.insert(finalName, math.random() < 0.5 and "/" or "\\")  
        end
    end

    return table.concat(finalName)
end

local name = distortName(player.Name, 0.2)  -- Shuffles 20% of the name


StarterGui:SetCore("SendNotification", {
    Title = "Executed!"; 
    Text = "Script Hub was Injected/Executed"; 
    Icon = "rbxassetid://126104158730275";  
    Duration = 5;  
    Button1 = "OK";
    Callback = function()
        print("Script Hub Injected")
    end
})





-- Hub
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hello, ["..name.."]", "BloodTheme")

local Tab = Window:NewTab("Built-in")
local Section = Tab:NewSection("cool stuff here")
Section:NewLabel("RNG")

local rngmin = 1
local rngmax = 10

Section:NewTextBox("Minimum", "Set the RNG minimum", function(txt)
    rngmin = tonumber(txt) or rngmin  
end)

Section:NewTextBox("Maximum", "Set the RNG maximum", function(txt)
    rngmax = tonumber(txt) or rngmax  
end)

Section:NewButton("RNG", "Activates the RNG", function()
    if rngmin > rngmax then
        StarterGui:SetCore("SendNotification", {
            Title = "Error"; 
            Text = "Minimum cannot be greater than Maximum!"; 
            Duration = 5;  
            Button1 = "OK";
        })
        return
    end

    local randomNumber = math.random(rngmin, rngmax)

    StarterGui:SetCore("SendNotification", {
        Title = tostring(randomNumber); 
        Text = "Your number is:"; 
        Icon = "rbxassetid://126104158730275";  
        Duration = 10;  
        Button1 = "OK";
        Callback = function()
            print("RNG activated")
        end
    })
end)




