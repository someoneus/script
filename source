local player = game.Players.LocalPlayer

local function distortName(str)
    local replacements = {o = "0", O = "0", l = "i", L = "I", e = "3", E = "3", s = "5", S = "5", a = "4", A = "4"}  

    local finalName = {}

    for i = 1, #str do
        local char = str:sub(i, i)

        -- Replace letters with chaotic counterparts
        if replacements[char] then
            char = replacements[char]
        end

        table.insert(finalName, char)
    end  -- Fixed misplaced 'end'

    return table.concat(finalName)
end


    local displayName = distortName(player.Character.Humanoid.DisplayName)
    local name = distortName(player.Name)

