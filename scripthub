Section:NewLabel("0")

local isGenerating = false  
Section:NewToggle("Fun Number", "Randomly generates numbers 1 to the power of 10", function(state)
    isGenerating = state  

    if isGenerating then
        print("Fun number started")

        task.spawn(function()
            local exponent = 0
            while isGenerating do
                local num = math.pow(10, exponent)
                exponent = exponent + 1  
                task.wait(0.5)  
                label:UpdateLabel(num)
            end
        end)

    else
        print("Fun number stopped")
    end
end)

